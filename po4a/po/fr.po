"include /usr/share/cdbs/1/rules/debhelper.mk\n"
"include /usr/share/cdbs/1/class/autotools.mk\n"
"\n"
msgstr ""
"[basicstyle=\\ttfamily\\footnotesize,escapeinside=\\{\\}]\n"
"#!/usr/bin/make -f\n"
"include /usr/share/cdbs/1/rules/debhelper.mk\n"
"include /usr/share/cdbs/1/class/autotools.mk\n"
"\n"

#. type: lstlisting
#: packaging-tutorial.tex:607
#, no-wrap
